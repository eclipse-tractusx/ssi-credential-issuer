{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "Org.Eclipse.TractusX.SsiCredentialIssuer": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "Org.Eclipse.TractusX.SsiCredentialIssuer.Reissuance.App"
    }
  },
  "ConnectionStrings": {
    "IssuerDb": ""
  },
  "Reissuance": {
    "ExpiredVcsToReissueInDays": 14,
    "CreatorUserId": ""
  },
  "ProcessIdentity": {
    "IdentityId": ""
  },
  "Portal": {
    "Username": "",
    "Password": "",
    "ClientId": "",
    "GrantType": "",
    "ClientSecret": "",
    "Scope": "",
    "KeycloakTokenAddress": "",
    "BaseAddress": ""
  },
  "Credential": {
    "IssuerDid": "",
    "IssuerBpn": "",
    "StatusListUrl": "",
    "StatusListType": "",
    "MaxPageSize": 15,
    "EncryptionConfigIndex": 0,
    "EncryptionConfigs": []
  }
}